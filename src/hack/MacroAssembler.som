MacroAssembler = Assembler (

  GOTO: sym = (
    self @ sym.
    self jump: JMP.
  )

  LET: sym EQ: val = (
    self @ sym.
    self M: val
  )

  MOV: src TO: dst = (
    self @ src.
    self D: 'M'.
    self @ dst.
    self M: 'D'
  )

  FOR: i FROM: start TO: end do: block = (
    self LET: i EQ: start.


  )

  "
   FOR: 'i' FROM: 1 TO: 10 do: [

   ]

  "
)
